package praticesJava;

public class BiggestNumber {
	public static void getBiggestNumber() {
		int a[] = { 5, 20, 5, 3, 60, 55 };

		int BiggestNumber = 0;

		for (int i = 0; i < a.length; i++) {
			
			if(a[i]>a[a.length-1]) {
				BiggestNumber = a[i];
			}
		}
		
		System.out.println(BiggestNumber);

		
	}
	
	public static void main(String[] args) {
		getBiggestNumber();
	}

}
